<div class="container">
	<h2>Historial Facturas</h2>
	
		<table class="table table-hover" >
					<th>Factura</th>
					<th>Fecha Pago</th>
					<th>Fecha Limite</th>
					<th>Cuentas Vencidas</th>
					<th>Valor</th>
					<th>Estado</th>
					<th colspan="2" class="text-center">Acciones</th>
					
					<% _.each(facturas, function(factura) { %>
					<tr>
						<td><%=factura.tipo%></td>
						<td><%=factura.fechaPago%></td>
						<td><%=factura.fechaLimite%></td>
						<td><%=factura.cuentasVencidas%></td>
						<td><%=factura.valor%></td>
						<td><%=factura.Estado%></td>
						<td>
							<center>
							<a href="/factura/show/<%=factura.id%>" class="btn btn-success btn-sm ">Detalles</a>
						
							<a href="/factura/updatestate/<%=factura.id%>" class="btn btn-success btn-sm ">Cambiar Estado</a>
							</center>
						</td>
						
					</tr>
					<%})%>
				</table>
				<a href="/usuario/history" class="btn btn-success btn-sm pull-right ">Atr√°s</a>
</div>
